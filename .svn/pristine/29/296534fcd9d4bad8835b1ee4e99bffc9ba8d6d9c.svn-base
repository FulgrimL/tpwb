    <script src="../js/jquery-2.1.4.js"></script>
    <script src="../js/fastclick.js"></script>
    <script src="../js/jquery-weui.js"></script>
    <script src="../js/mui.min.js"></script>
    <script>
    	$(function() {
            var gsuserId=localStorage.getItem("gsuserId");
           /* var GsuserId=localStorage.getItem("gsuserId");*/
            console.log(gsuserId);
			var postData = {
				gsuserId: gsuserId
			};
			mui.ajax({
				url: httpLocation2+"/tpwb/customers/getCustomers",
				type: 'POST',
				data: JSON.stringify(postData),
				contentType: "application/json;charset=utf-8",
				dataType: 'json',
				success: function(backdata) {
					var resData = backdata.data;
					var htmlStr = " ";
					for(var i in resData) {
                        htmlStr += '<li id=' + resData[i].customerId + ' onclick=tiaozhuan('+resData[i].customerId+') >';
                        htmlStr += '<span class="fz" >'+resData[i].customerName+'</span>';
                        htmlStr += '<i class="iconfont">' +'&#xe636;' + '</i>' + '</li>';
					}
					$("#cardList").append(htmlStr);
				},
			})

		});
		
		 
        $(".fixSure").click(function() {
            window.location.href = "addCustomer.html";
        })

        function tiaozhuan(customerId){
            window.location.href = "productSelect.html?CustomerId="+customerId;

        }
    </script>
